{
    "extends": ["airbnb", "prettier", "prettier/react"],
    "plugins": ["prettier", "react"],
    "rules": {
        "indent": ["error", 4],
        "react/destructuring-assignment": 1,
        "react/jsx-filename-extension": 1,
        "import/no-dynamic-require": 0
    },
    "globals": {
        "window": true,
        "document": true,
        "localStorage": true,
        "FormData": true,
        "FileReader": true,
        "Blob": true,
        "navigator": true,
        "fetch": true
    },
    "parser": "babel-eslint"
}
